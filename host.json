{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "hubName": "testhub2",
      "storageProvider": {
        "partitionCount": 1
      },
      "maxConcurrentActivityFunctions": 1,
      "maxConcurrentOrchestratorFunctions": 1
    }
  },
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    }
}